<p id="notice"><%= notice %></p>

<h1>Listing Enrolments</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Delivery mode</th>
      <th>Student</th>
      <th>Address</th>
      <th>Employer</th>
      <th>Emergeny contact</th>
      <th>Language culture diversity</th>
      <th>School</th>
      <th>Employment</th>
      <th>Vic student</th>
      <th>Completed quqlification</th>
      <th>Qualication received location</th>
      <th>Vet</th>
      <th>Toc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enrolments.each do |enrolment| %>
      <tr>
        <td><%= enrolment.course %></td>
        <td><%= enrolment.delivery_mode %></td>
        <td><%= enrolment.student %></td>
        <td><%= enrolment.address %></td>
        <td><%= enrolment.employer %></td>
        <td><%= enrolment.emergency_contact %></td>
        <td><%= enrolment.language_culture_diversity %></td>
        <td><%= enrolment.school %></td>
        <td><%= enrolment.employment %></td>
        <td><%= enrolment.vic_student %></td>
        <td><%= enrolment.completed_qualification %></td>
        <td><%= enrolment.qualification_received_location %></td>
        <td><%= enrolment.vet %></td>
        <td><%= enrolment.toc %></td>
        <td><%= link_to 'Show', enrolment %></td>
        <td><%= link_to 'Edit', edit_enrolment_path(enrolment) %></td>
        <td><%= link_to 'Destroy', enrolment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enrolment', new_enrolment_path %>
